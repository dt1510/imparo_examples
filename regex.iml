%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
% Regex learning problem. 
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

% These parameters can be varied to control the form of the hypotheses learned.
:-set_max_clause_length(5).
:-set_max_clauses(1).
:-set_connected(1).
:-set_max_var_depth(3).

head_modes([
    p(+word)
]).

body_modes([
    p(+word)
]).

word(e).
word(a(b(e))).
word(a(b(a(b(e))))).
word(p(a(b(a(b(a(b(e)))))))).
word(p(b(e))).
word(p(a(e))).
word(p(a(a(b(e))))).

